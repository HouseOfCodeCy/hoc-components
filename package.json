{
  "name": "@hoc/hoc-components",
  "version": "1.0.3",
  "description": "A component library for HoC",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "repository": "git@github.com:HouseOfCodeCy/hoc-components.git",
  "author": "c_mario7@hotmail.com",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "build": "rm -rf ./dist && tsc && yarn run postbuild",
    "postbuild": "yarn build-sass && yarn copy-files && yarn copy-sass",
    "build-sass": "sass --load-path=./node_modules --load-path=./ src/styles/theme.scss dist/styles/theme.scss",
    "copy-files": "copyfiles -f package.json dist",
    "copy-sass": "copyfiles -f src/styles/**/* -e src/styles/theme.scss dist/styles"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT",
  "devDependencies": {
